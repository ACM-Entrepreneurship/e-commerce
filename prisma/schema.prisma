datasource db {
  provider = "postgresql"
  url      = env("DB_URL_BjOXW6ZuMU4DOUWIicsAeo7gbqxmJD1tbVDSE9dTPUE2+aehj5uJpPOqmzEz61gU")
}

generator client {
  provider = "prisma-client-js"
}

model Testing {
  id         BigInt    @id @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}

model User {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email String @unique
  image String?
  role Role @default(USER)
}

enum Role {
  ADMIN
  USER
  VENDOR
}

model Product {
  id String @id @default(uuid())
  createdAt DateTime @updatedAt
  title String
  description String
  url String
  imageUrl String
  category String
}